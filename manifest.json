{
  "manifest_version": 2,
  "name": "WebTracker",
  "version": "1.0",
  "description": "在网页上添加和管理标注",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content/content.css"],
      "js": ["content/content.js"]
    }
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 